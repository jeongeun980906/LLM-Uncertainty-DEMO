<mujoco model="dell">
  <compiler meshdir="./" fitaabb="true" />

  <visual>
    <headlight diffuse="0.6 0.6 0.6" ambient="0.1 0.1 0.1" specular="0 0 0" />
    <rgba haze="0.15 0.25 0.35 1" />
    <global azimuth="120" elevation="-20" />
  </visual>

  <asset>
    <!-- <mesh name="dell" file="dell_2.obj" scale="1 1 1" /> -->
    <mesh name="dell" file="dell.obj" scale="1 1 1" />

    <!-- Sky -->
    <texture type="skybox" builtin="gradient" rgb1="0.1 0.2 0.3" rgb2="0 0 0" width="512"
      height="3072" />
    <!-- Ground -->
    <texture type="2d" name="groundplane" builtin="checker" mark="edge" rgb1="0.3 0.4 0.5"
      rgb2="0.1 0.2 0.3" markrgb="0.8 0.8 0.8" width="300" height="300" />
    <material name="groundplane" texture="groundplane" texuniform="true" texrepeat="2 2"
      reflectance="0.2" />
  </asset>

  <worldbody>
    <light pos="0 0 3" dir="0 0 -1" directional="true" diffuse="1 1 1" />
    <geom name="floor" size="0 0 0.05" type="plane" material="groundplane" />

    <body name="dell" pos="0 0 2" quat="0 0 1 1" >
      <joint name="dell_free" type="free" />
      <geom mesh="dell" type="mesh" condim="3" contype="1" conaffinity="1" group="2" rgba="0.1 0.1 0.1 1"/>
    </body>
  </worldbody>
</mujoco>